  function startapp() {
    url = window.location.href;
    var hash = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
    var language = "en";
    var scb_token = "";
    var source = "";
    var offerCode = "";
    for (var i = 0; i < hash.length; i++) {
      if (hash[i].indexOf("scbAccessToken") != -1) {
        scb_token = hash[i].split('=')[1];
      }
      else if (hash[i].indexOf("source") != -1) {
        source = hash[i].split('=')[1];
      }
      if (hash[i].indexOf("lang") != -1) {
        language = hash[i].split('=')[1];
      }
      if (hash[i].indexOf("offer") != -1) {
        offerCode = hash[i].split('=')[1];
      }
    }
    if (scb_token != null) {
      sessionStorage.setItem("landingURL", window.location.href);
    }
    var rand = Math.floor((1 + Math.random()) * 0x10000);
    //T0 Product
    if (x == "t0") {
      if (scb_token.length > 0) { //With SCB token
        if (offerCode == "A00") {
          window.open("/onboarding/hk-uat/www/rtoForms/index.html?rand=" + rand + "#/onboarding?country=HK&sourceName=asiamiles&products={'product_category':'CC','product_type':'1313','campaign':'HKSOZ00MCWB000','name':'Standard Chartered Cathay Mastercard – Priority Banking'}&channelName=ibk_pre_am&cardType=3268&affiliation=2132&pageId=pd-1&premium=CT00&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A01") {
          window.open("/onboarding/hk-uat/www/rtoForms/index.html?rand=" + rand + "#/onboarding?country=HK&sourceName=asiamiles&products={'product_category':'CC','product_type':'1313','campaign':'HKSOZ00MCWB000','name':'Standard Chartered Cathay Mastercard'}&channelName=ibk_pre_am&cardType=3268&affiliation=2132&premium=CO00&pageId=pd-1&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A02") {
          window.open("/onboarding/hk-uat/www/rtoForms/index.html?rand=" + rand + "#/onboarding?country=HK&sourceName=asiamiles&products={'product_category':'CC','product_type':'1313','campaign':'HKSOZ00MCWB000','name':'Standard Chartered Cathay Mastercard'}&channelName=ibk_pre_am&cardType=3268&affiliation=2132&premium=CT30&pageId=pd-1&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A03") {
          window.open("/onboarding/hk-uat/www/rtoForms/index.html?rand=" + rand + "#/onboarding?country=HK&sourceName=asiamiles&products={'product_category':'CC','product_type':'1313','campaign':'HKSOZ00MCWB000','name':'Standard Chartered Cathay Mastercard(CT00)'}&channelName=ibk_pre_am&cardType=3268&affiliation=2132&premium=CT00&pageId=pd-1&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A04") {
          window.open("/onboarding/hk-uat/www/rtoForms/index.html?rand=" + rand + "#/onboarding?country=HK&sourceName=asiamiles&products={'product_category':'CC','product_type':'1313','campaign':'HKSOZ00MCWB000','name':'Standard Chartered Cathay Mastercard'}&channelName=ibk_pre_am&cardType=3268&affiliation=2132&premium=CT40&pageId=pd-1&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
        }
        else if (offerCode == "A05") {
          window.open("/onboarding/hk-uat/www/rtoForms/index.html?rand=" + rand + "#/onboarding?country=HK&sourceName=asiamiles&products={'product_category':'CC','product_type':'1313','campaign':'HKSOZ00MCWB000','name':'Standard Chartered Cathay Mastercard'}&channelName=ibk_pre_am&cardType=3268&affiliation=2132&premium=CT70&pageId=pd-1&scbAccessToken=" + scb_token + "&lang=" + language + "&source=" + source, "_self");
