  <script>
    function getDeviceType() {
      let device = null;
      if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
        device = "App";
      } else {
        device = "Web";
      }
      return device;
    }
    function viewStart(stepName) {
      stepName = stepName ? stepName : 'na';
      let productName = 'na'; //appData.applicationdata.products[0].name
      let productID = 'na'; //appData.applicationdata.products[0].product_type
      let productCategory = 'na'; //appData.applicationdata.products[0].product_category
      window.adobeDataLayer.push({
        "event": "at-view-start",
        "page": {
          "pageInfo": {
            "pageName": 'HK' + ":" + 'EN' + ":" + 'personal' + ":" + 'form' + ":" + productCategory + ":"
              + 'na' + ":" + productName + ":"
              + 'HK_CCPL' + ":" + stepName, //Same page name as Analytics
            "viewName": stepName,
            "primaryCategory": productCategory,
            "subCategory1": "na"
          }
        }
      });
    }
    function pageView(stepName) {
      stepName = stepName ? stepName : 'na';
      let productName = 'na'; //appData.applicationdata.products[0].name
      let productID = 'na'; //appData.applicationdata.products[0].product_type
      let productCategory = 'na'; //appData.applicationdata.products[0].product_category

      window.adobeDataLayer.push({
        event: "pageView",
        page: {
          pageInfo: {
            pageType: 'form',
            // {country}:{language}:{segment}:{page type}:{product category}:{product sub-category}:{product name}:{form name}:{Screen Name}
            pageName: 'HK' + ":" + 'EN' + ":" + 'personal' + ":" + 'form' + ":" + productCategory + ":"
              + 'na' + ":" + productName + ":"
              + 'HK_CCPL' + ":" + stepName,
            buildDetails: '79.0.0',
            libDetails: 'prod'
          },
          category: {
            primaryCategory: productCategory,
            subCategory1: 'na'
          },
          attributes: {
            country: 'HK',
            language: 'EN',
            platform: getDeviceType(),
            pfm: 'na'
          },
        },
        form: {
          formName: 'HK_CCPL',
          formStepName: stepName,
          formPlatform: 'rtob',
          formType: 'application',
          formFields: [{
            formFieldValue: "na",
            formFieldName: "na"
          }]
        },
        user: {
          "userInfo": {
            userID: 'na',
            userType: 'na',
            userStatus: 'na',
            segment: 'personal'
          }
        },
        product: [{
          productInfo: {
            productName: productName,
            productID: productID,
            productCategory: productCategory,
            productSubCategory: 'na',
          }
        }]
      });

    }

    function ctaClick(stepName, buttonName, productName) {
      // var step_title = title[stepName];
      stepName = stepName ? stepName : 'na';
      productName = productName ? productName : 'na'; //appData.applicationdata.products[0].name
      let productID = '1313'; //appData.applicationdata.products[0].product_type
      let productCategory = 'CC'; //appData.applicationdata.products[0].product_category

      window.adobeDataLayer.push({
        event: "ctaClick",
        page: {
          pageInfo: {
            pageType: 'form',
            // {country}:{language}:{segment}:{page type}:{product category}:{product sub-category}:{product name}:{form name}:{Screen Name}
            pageName: 'HK' + ":" + 'EN' + ":" + 'personal' + ":" + 'form' + ":" + productCategory + ":"
              + 'na' + ":" + productName + ":"
              + 'HK_CCPL' + ":" + stepName,
            buildDetails: '79.0.0',
            libDetails: 'uat'
          },
          category: {
            primaryCategory: productCategory,
            subCategory1: 'na'
          },
          attributes: {
            country: 'HK',
            language: 'EN',
            platform: getDeviceType()
          },
        },
        form: {
          formName: productCategory === 'CC' || productCategory === 'PL' ? 'HK_CCPL' : 'HK_CASA',
          formStepName: stepName,
          formPlatform: 'rtob',
          formType: 'application',
          formFields: [{ formFieldValue: "na", formFieldName: "na" }]
        },
        customLinkClick: {
          customLinkText: buttonName,
          customLinkRegion: buttonName === 'Login' ? 'top' : 'bottom',
          // customLinkType: buttonName === 'Continue to Login' || 'Login to Online Banking' ? 'button' : 'link'
          customLinkType: 'button'
        },
        user: {
          "userInfo": {
            userID: 'na',
            userType: 'na',
            userStatus: 'na',
            segment: 'personal'
          }
        },
        product: [{
          productInfo: {
            productName: productName,
            productID: productID,
            productCategory: productCategory,
            productSubCategory: 'na',
          }
        }],
      });
    }
    viewStart("Asiamiles");
    pageView("Asiamiles");
  </script>
