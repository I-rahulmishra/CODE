  // LoadJson 2nd - To Load cards_variants Json.
  function loadJSON(callback, lan, offerC) {
    var hash = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
    var lan = "en";

    for (var i = 0; i < hash.length; i++) {
      if (hash[i].indexOf("lang") != -1) {
        lan = hash[i].split('=')[1];
      }
    }

    for (var i = 0; i < hash.length; i++) {
      if (hash[i].indexOf("offer") != -1) {
        offerC = hash[i].split('=')[1];
      }
    }

    var xobj = new XMLHttpRequest();
    xobj.overrideMimeType("application/json");
    if (lan === "zh") {
      xobj.open('GET', 'assets/json/cards_variants_c.json', true);
    }
    else if (lan === "en") {
      xobj.open('GET', 'assets/json/cards_variants_e.json', true);
    }
    xobj.onreadystatechange = function () {
      if (xobj.readyState == 4 && xobj.status == "200") {
        if (offerC) {
          callback(JSON.parse(xobj.responseText), lan, offerC);
        }
        else {
          callback(JSON.parse(xobj.responseText), lan, "A00");
        }
      }
    };
    xobj.send(null);
  }

  // For Selct Button Click 3rd
  function myFunction(clickid, flag) {

    document.getElementById("nextBtnTxt").disabled = false;
    var t = document.getElementById(clickid).value
    var hash = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');

    for (var i = 0; i < hash.length; i++) {
      if (hash[i].indexOf("lang") != -1) {
        lan = hash[i].split('=')[1];
      }
    }

    if (t == "t0") {
      x = document.getElementById("myCheck").value;
      y = 0;
      z = 0;

      // disable other buttons
      if (lan === "en") {
        document.getElementById("myCheck2").innerHTML = 'Select';
        document.getElementById("myCheck1").innerHTML = 'Select';
      } else if (lan === "zh") {
        document.getElementById("myCheck2").innerHTML = '選擇';
        document.getElementById("myCheck1").innerHTML = '選擇';
      }
      // disable hover effect of other button
      var elem1 = document.getElementById("myCheck1");
      var elem2 = document.getElementById("myCheck2");
      var elem3 = document.getElementById("nextBtn");
      $(elem1).removeClass('button');
      $(elem2).removeClass('button');
      $(elem3).removeClass('greyed');
      elem3.style.background = '#4CAF50';
      elem3.style.color = '#fff';
      elem3.style.display = 'block';
      elem3.style.padding = '10px 15px';
      elem3.style.cursor = 'pointer';
      var elem = document.getElementById("myCheck");
      if (lan === "en") {
        elem.innerHTML = '<img id ="img1"  src="assets/images/check1.png" /> Selected';
      } else if (lan === "zh") {
        elem.innerHTML = '<img id ="img1"  src="assets/images/check1.png" />  已選擇	';
      }
      elem.style.background = '#ffffff';
      elem.style.color = '#4CAF50';
      elem.style.cursor = 'inherit';
      var img = document.getElementById("img1");
      img.style.cursor = 'pointer';
      elem.style.cursor = 'pointer';
    }
    else if (t == "t1") {
      x = 0;
      y = document.getElementById("myCheck1").value;
      z = 0;

      // disable other buttons
      if (lan === "en") {
        document.getElementById("myCheck").innerHTML = 'Select';
        document.getElementById("myCheck2").innerHTML = 'Select';
      } else if (lan === "zh") {
        document.getElementById("myCheck").innerHTML = '選擇';
        document.getElementById("myCheck2").innerHTML = '選擇';
      }

      // disable hover effect of other button
      var elem = document.getElementById("myCheck");
      var elem2 = document.getElementById("myCheck2");
      var elem3 = document.getElementById("nextBtn");
      $(elem).removeClass('button');
      $(elem2).removeClass('button');
      $(elem3).removeClass('greyed');
      elem3.style.background = '#4CAF50';
      elem3.style.color = '#fff';
      elem3.style.display = 'block';
      elem3.style.padding = '10px 15px';
      elem3.style.cursor = 'pointer';
      var elem1 = document.getElementById("myCheck1");
      if (lan === "en") {
        elem1.innerHTML = '<img id ="img2"  src="assets/images/check1.png" /> Selected';
      } else if (lan === "zh") {
        elem1.innerHTML = '<img id ="img2"  src="assets/images/check1.png" /> 已選擇	';

      }
      elem1.style.background = '#ffffff';
      elem1.style.color = '#4CAF50';
      elem1.style.cursor = 'inherit';
      var img = document.getElementById("img2");
      img.style.cursor = 'pointer';
      elem1.style.cursor = 'pointer';
    }
